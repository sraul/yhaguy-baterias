<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//hibernate/hibernate Mapping DTD 3.0//EN"
        "classpath://org/hibernate/hibernate-mapping-3.0.dtd"
        [ <!ENTITY hibernate-domain SYSTEM "classpath://com/coreweb/hibernate-domain.hbm.xml"> ]
        >

<hibernate-mapping>

	<class name="com.yhaguy.domain.ColectorDatos" table="colectordatos">

		&hibernate-domain; 
		
		<property name="nroDocumento"/>
		<property name="tipoDocumento"/>
		<property name="nombreApellido"/>
		<property name="razonSocialEmpresa"/>
		<property name="rucEmpresa"/>
		<property name="telefono"/>
		<property name="correo"/>
		
	</class>
</hibernate-mapping>
